//	MAC(Macro suffix,	Pattern,					Debug name,				Display name)
	MAC(IDENTIFIER,		"<[a-zA-Z0-9_\\.\\-]+>",	"Identifier",			"<Identifier>")
	MAC(GLSL_IDENT,		"\\[[a-zA-Z_]\\w*\\]",		"Shader Identifier",	"[Shader_Identifier]")
	MAC(ENUMERATOR,		"\\{[a-zA-Z_\\-]\\w*\\}",	"Enumerator",			"{Enumerator}")
	MAC(FILENAME,		"\\\"[^\\\"\\n\\t]+\\\"",	"Filename",				"\"Filename String\"")
	MAC(STORE_NAME,		"\\\'[^\\\'\\n\\t]+\\\'",	"Stored Name",			"'Stored Name String'")
