    <refnamediv>
        <refname>gluNewQuadric</refname>
        <refpurpose>create a quadrics object</refpurpose>
    </refnamediv>
    <refsynopsisdiv>
        <funcsynopsis>
            <funcprototype>
                <funcdef>GLUquadric* <function>gluNewQuadric</function></funcdef>
            </funcprototype>
